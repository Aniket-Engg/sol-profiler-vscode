{
	"name": "sol-profiler-vscode",
	"displayName": "sol-profiler",
	"description": "Create Smart Contracts Attributes Profile",
	"version": "1.0.0",
	"publisher": "Aniket-Engg",
	"repository": "https://github.com/Aniket-Engg/sol-profiler-vscode",
	"engines": {
		"vscode": "^1.26.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.sol-profiler.generate"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.sol-profiler.generate",
				"title": "sol-profiler: generate profile"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"solparse": "^2.2.8",
		"table": "^5.2.3"
	}
}
